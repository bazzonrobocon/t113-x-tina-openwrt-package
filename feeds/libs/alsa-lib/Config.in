# alsa package config

menu "Configuration"
	depends on PACKAGE_alsa-lib

config ALSA_LIB_EXTRA_resmgr
	bool "Compile to support resmgr"
	default n
	help
	  resmgr -

config ALSA_LIB_EXTRA_aload
	bool "Support reading /dev/aload*"
	default n
	help
	  aload -

config ALSA_LIB_EXTRA_mixer
	bool "Enable the mixer component"
	default y
	help
	  mixer -

config ALSA_LIB_EXTRA_pcm
	bool "Enable the pcm component"
	default y
	help
	  pcm -

choice
    prompt "compile pcm plugins"
        depends on ALSA_LIB_EXTRA_pcm
        default compile-all-pcm-plugins
        help
            choose compile pcm plugins
        config compile-all-pcm-plugins
            bool "all"
        config custom-pcm-plugins
            bool "custom"
endchoice
menu "Select PCM Plugins"
    depends on custom-pcm-plugins
